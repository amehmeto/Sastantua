{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "sourceMap": true,
    "outDir": "build",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*",
    "build/Bowling.d.ts"
  ]
}